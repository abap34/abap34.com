{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$ref": "#/$defs/JETLS.JETLSConfig__5f2736aac9cfedc3",
    "$defs": {
        "Bool__5d3b799b1e2eefc8": {
            "type": "boolean"
        },
        "Float64__86b4ee2e5e69187d": {
            "type": "number"
        },
        "Glob.FilenameMatch{String}__ba2567238955a0b5": {
            "type": "string"
        },
        "String__f000e81cec2244bc": {
            "type": "string"
        },
        "JETLS.JETLSConfig__5f2736aac9cfedc3": {
            "properties": {
                "diagnostic": {
                    "properties": {
                        "enabled": {
                            "$ref": "#/$defs/Bool__5d3b799b1e2eefc8",
                            "description": "Enable or disable all JETLS diagnostics. When set to false, no diagnostic messages will be shown."
                        },
                        "patterns": {
                            "items": {
                                "properties": {
                                    "match_by": {
                                        "type": "string",
                                        "description": "What to match against: 'code' matches against diagnostic codes (e.g., 'lowering/unused-argument'), 'message' matches against diagnostic message text.",
                                        "enum": [
                                            "code",
                                            "message"
                                        ]
                                    },
                                    "pattern": {
                                        "anyOf": [
                                            {
                                                "format": "regex",
                                                "type": "string"
                                            },
                                            {
                                                "$ref": "#/$defs/String__f000e81cec2244bc"
                                            }
                                        ],
                                        "description": "The pattern to match (e.g., 'lowering/unused-argument' for code matching, or 'Macro name .*' for message matching)."
                                    },
                                    "severity": {
                                        "oneOf": [
                                            {
                                                "minimum": 0,
                                                "type": "integer",
                                                "maximum": 4
                                            },
                                            {
                                                "type": "string",
                                                "enum": [
                                                    "off",
                                                    "error",
                                                    "warning",
                                                    "warn",
                                                    "information",
                                                    "info",
                                                    "hint"
                                                ]
                                            }
                                        ],
                                        "description": "Severity level: 'error'/1 for critical issues, 'warning'/'warn'/2 for potential problems, 'information'/'info'/3 for informational messages, 'hint'/4 for suggestions, 'off'/0 to disable."
                                    },
                                    "match_type": {
                                        "type": "string",
                                        "description": "How to interpret the pattern: 'literal' for exact string match, 'regex' for regular expression match.",
                                        "enum": [
                                            "literal",
                                            "regex"
                                        ]
                                    },
                                    "path": {
                                        "$ref": "#/$defs/Glob.FilenameMatch{String}__ba2567238955a0b5",
                                        "description": "Optional glob pattern to restrict this configuration to specific files (e.g., 'test/**/*.jl'). Patterns are matched against file paths relative to the workspace root. Supports globstar (**) for matching directories recursively."
                                    }
                                },
                                "required": [
                                    "pattern",
                                    "match_by",
                                    "match_type",
                                    "severity"
                                ],
                                "additionalProperties": false,
                                "type": "object"
                            },
                            "type": "array",
                            "description": "Fine-grained control over diagnostics through pattern matching. See [Pattern-based diagnostic configuration](https://aviatesk.github.io/JETLS.jl/release/configuration/#config/diagnostic/patterns)."
                        }
                    },
                    "additionalProperties": false,
                    "type": "object",
                    "description": "Diagnostic configuration. See [Diagnostic configuration](https://aviatesk.github.io/JETLS.jl/release/configuration/#config/diagnostic)."
                },
                "full_analysis": {
                    "properties": {
                        "auto_instantiate": {
                            "$ref": "#/$defs/Bool__5d3b799b1e2eefc8",
                            "description": "When enabled, JETLS automatically runs Pkg.instantiate() for packages that have not been instantiated yet (e.g., freshly cloned repositories). This allows full analysis to work immediately upon opening such packages. Note that this will automatically create a Manifest.toml file when the package has not been instantiated yet."
                        },
                        "debounce": {
                            "$ref": "#/$defs/Float64__86b4ee2e5e69187d",
                            "description": "Debounce time in seconds before triggering full analysis after a document change."
                        }
                    },
                    "additionalProperties": false,
                    "type": "object",
                    "description": "Configuration for full JET analysis. See [Full analysis configuration](https://aviatesk.github.io/JETLS.jl/release/configuration/#config/full_analysis)."
                },
                "formatter": {
                    "oneOf": [
                        {
                            "type": "string",
                            "enum": [
                                "JuliaFormatter",
                                "Runic"
                            ]
                        },
                        {
                            "properties": {
                                "custom": {
                                    "properties": {
                                        "executable_range": {
                                            "type": "string"
                                        },
                                        "executable": {
                                            "type": "string"
                                        }
                                    },
                                    "required": [
                                        "executable"
                                    ],
                                    "additionalProperties": false,
                                    "type": "object"
                                }
                            },
                            "required": [
                                "custom"
                            ],
                            "additionalProperties": false,
                            "type": "object"
                        }
                    ],
                    "description": "Formatter configuration. Can be a preset name ('Runic' or 'JuliaFormatter') or a custom formatter object. See [Formatting configuration](https://aviatesk.github.io/JETLS.jl/release/configuration/#config/formatter)."
                },
                "testrunner": {
                    "properties": {
                        "executable": {
                            "$ref": "#/$defs/String__f000e81cec2244bc",
                            "description": "Path to the TestRunner.jl executable. If not set, uses 'testrunner' from PATH."
                        }
                    },
                    "additionalProperties": false,
                    "type": "object",
                    "description": "TestRunner integration configuration. See [TestRunner integration](https://aviatesk.github.io/JETLS.jl/release/configuration/#config/testrunner)."
                },
                "initialization_options": {
                    "properties": {
                        "analysis_overrides": {
                            "items": {
                                "properties": {
                                    "module_name": {
                                        "$ref": "#/$defs/String__f000e81cec2244bc"
                                    },
                                    "path": {
                                        "$ref": "#/$defs/Glob.FilenameMatch{String}__ba2567238955a0b5"
                                    }
                                },
                                "required": [
                                    "path"
                                ],
                                "additionalProperties": false,
                                "type": "object"
                            },
                            "type": "array"
                        },
                        "n_analysis_workers": {
                            "minimum": -9223372036854775808,
                            "type": "integer",
                            "maximum": 9223372036854775807,
                            "description": "Number of analysis worker tasks. Default is 1. See [documentation](https://aviatesk.github.io/JETLS.jl/release/launching/#init-options/n_analysis_workers) for details and current limitations."
                        }
                    },
                    "additionalProperties": false,
                    "type": "object",
                    "description": "Static initialization options sent to JETLS during startup. These settings require a server restart to take effect."
                }
            },
            "additionalProperties": false,
            "type": "object"
        }
    }
}
